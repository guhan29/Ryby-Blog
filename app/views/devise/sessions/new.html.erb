<div class="row">
  <div class="col-md-5 col-md-offset-3">
    <div class="well myform">
      <fieldset>
        <legend>Log in</legend>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="field form-group">
            <%= f.label :email %><br />
            <%= f.email_field :email, class: "form-control", autofocus: true, autocomplete: "email" %>
          </div>

          <div class="field form-group">
            <%= f.label :password %><br />
            <%= f.password_field :password, class: "form-control", autocomplete: "current-password" %>
            <br>
            <%= link_to "Forgot Password?", new_user_password_path %>
          </div>

          <% if devise_mapping.rememberable? -%>
            <div class="field form-group">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
            </div>
          <% end -%>

          <div class="actions">
            <%= f.submit "Log in", :class => "btn btn-info btn-block" %>
          </div>
          <br>
          <p><span>Don't have account?</span> <%= link_to "Sign Up", new_user_registration_path %> </p>
        <% end %>
      </fieldset>
      <!-- < render "devise/shared/links" > -->
    </div>
  </div>
</div>

